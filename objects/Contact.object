    <validationRules>
        <fullName>Contact_must_be_in_Account_ZIP_Code</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  NOT(ISBLANK( AccountId)),
   Account.ShippingPostalCode !=  MailingPostalCode 
)</errorConditionFormula>
        <errorDisplayField>MailingPostalCode</errorDisplayField>
        <errorMessage>Contact must be in Account ZIP Code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>email_or_phone_must_be_required</fullName>
        <active>false</active>
        <description>test edit2</description>
        <errorConditionFormula>ISBLANK( Email) &amp;&amp; ISBLANK( Phone )</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Email or phone are required !</errorMessage>
    </validationRules>
